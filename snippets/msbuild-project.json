		"prefix": "nuget-properties",
		"body": [
			"<PropertyGroup>",
			"    <PackageId>${1:PackageId}</PackageId>",
			"    <Title>${1}</Title>",
			"    <Version>${2:1.0.0}</Version>",
			"    <Authors>${3:Your Name}</Authors>",
			"    <Company>${4:MyCompany Inc}</Company>",
			"    <Product>${5:Product Name}</Product>",
			"    <Description>${6:Package Description}</Description>",
            "    <Copyright>${7:Â© MyCompany Corporation.  All rights reserved.}</Copyright>",
            
			"    <PackageLicenseUrl>${8:http://Your.License.Url}</PackageLicenseUrl>",
			"    <PackageProjectUrl>${9:http://Your.Project.Url}</PackageProjectUrl>",
            "    <PackageIconUrl>${10:http://Your.Icon.Url}</PackageIconUrl>",
			"    <PackageTags>${11:space-separated tag list}</PackageTags>",
			"    <PackageReleaseNotes>${12:http://Your.ReleaseNotes.Url}</PackageReleaseNotes>",
			"    <PackageRequireLicenseAcceptance>${13|false,true|}</PackageRequireLicenseAcceptance>",
            
			"    <RepositoryUrl>${14:https://github.com/user/repo}</RepositoryUrl>",
            "    <RepositoryType>${15:git}</RepositoryType>",

